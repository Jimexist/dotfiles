"*":
  core:
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".flowconfig"
      ]
      "source.json": [
        ".arcconfig"
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
  editor:
    invisibles:
      cr: "↩"
      tab: "⇥"
    showIndentGuide: true
    showInvisibles: false
    tabLength: 2
  "exception-reporting":
    userId: "ce3eaeef-bd6f-efd2-ac59-a638eb7ab826"
  "file-watcher":
    promptWhenFileHasChangedOnDisk: false
  "ide-haskell": {}
  linter: {}
  minimap: {}
  nuclide:
    installRecommendedPackages: true
    "nuclide-distraction-free-mode": {}
    "nuclide-home":
      showHome: false
  "tabs-to-spaces":
    onSave: "untabify"
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
