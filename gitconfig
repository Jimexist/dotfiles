[user]
    name = Jiayu Liu
    email = etareduce@gmail.com
[push]
    default = simple
    followTags = true
[transfer]
    fsckobjects = true
[merge]
    ff = only
    conflictstyle = diff3
[alias]
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
    prune = fetch --prune
[filter "media"]
    required = true
    clean = git media clean %f
    smudge = git media smudge %f
[filter "hawser"]
    clean = git hawser clean %f
    smudge = git hawser smudge %f
    required = true
[filter "lfs"]
    clean = git-lfs clean %f
    smudge = git-lfs smudge %f
    required = true
[core]
    autocrlf = input
[credential]
    helper = osxkeychain
